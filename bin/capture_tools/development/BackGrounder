#!/usr/bin/perl
# BackGrounder
use strict;
use warnings;
use Getopt::Long;
use Config::Std;
use Parallel::ForkManager;

use Data::Dumper;

my $usage = "

	*****STILL IN DEVELOPMENT*******


Synopsis:
	./BackGrounder --run reducereads --cfg configure/capture.cfg --dir reducereads/ --population CEU --cpu 8 
	./BackGrounder --run complete --cfg configure/capture.cfg --dir reducereads/ --population ALL --cpu 8 
	./BackGrounder --run collect --dir ncbi_bams/ 
	./BackGrounder --run view --dir ncbi_bams/ --population CEU

Description:
	Backgrounder is designed to do a couple of  things. 
	1 - Collect all 1000Genomes ftp bwa exome bams
	2 - Run GATK ReducedReads on exomes file to allow quicker use with variant calling.

	** Important to use the same fasta file used in analysis. **
 
Required
	--dir|d         : Path to directory of BAM files, to use or write to.
	--cfg|cf        : Configure file used for cApTUrE run. Script will use fasta/GATK information
	--run|r         : Specify type of run user want to perform.
			
			reducereads - Reduce the reads of a downloaded set of bam files.
			complete    - Download via ftp and reduce bam file from NCBI.
			collect     - Just download desired set of bam files.
			view        - Print to screen the list of desired bam files.
				

Options:
	--population|p  : 1000 Genomes population user would like to work with.
			  (http://www.1000genomes.org/about#ProjectSamples). 
                          Defalut is ALL
	--cpu|c	        : Number of CPUs used to split runs across. 
		          Does not use cpu info from config file.
                          Default 1.

	--help|h        : Print this usage statement.

\n";

my ( @dirs, $cpu, $cfg, $pop, $run, $help );
GetOptions(
    "dir|d=s" => \@dirs,
    "cpu|c=i"         => \$cpu,
    "config|cfg=s"    => \$cfg,
    "run|r=s"         => \$run,
    "population|p=s"    => \$pop,
    "help|h"          => \$help,
);
if (! $run and ! @dirs and ! $cfg or $help ) { die "$usage\n" }

# Information from config file
read_config $cfg => my %cf;
my $fasta = $cf{'required'}{'fasta'}    || die "fasta file not found\n";
my $gatk  = $cf{'software'}{'gatk'}     || die "GATK program not found\n";
my $tmp   = $cf{'required'}{'java_tmp'} || '/tmp';

# set population
unless ($pop) { $pop = 'ALL' }

# set up PF
unless ($cpu) { $cpu = 1 }
my $pm = Parallel::ForkManager->new($cpu);


#reduceRunner();
#wget()
#samIndex();


# dispatch table
my $steps = {
	reducereads => \&reducereads,
	complete    => \&complete,
	collect     => \&collect,
	view        => \&view,
};

$steps->{$run}->();

my $thousand = {
	CDX => '1',
	CHB => '1',
	JPT => '1',
	KHV => '1',
	CHS => '1',
	ASN => '1',
	BEB => '1',
	GIH => '1',
	ITU => '1',
	PJL => '1',
	STU => '1',
	SAN => '1',
	ASW => '1',
	ACB => '1',
	ESN => '1',
	GWD => '1',
	LWK => '1',
	MSL => '1',
	YRI => '1',
	ARF => '1',
	GBR => '1',
	FIN => '1',
	IBS => '1',
	TSI => '1',
	CEU => '1',
	EUR => '1',
	CLM => '1',
	MXL => '1',
	PEL => '1',
	AMR => '1',
};

print $thousand, "\n";

##-----------------------------------------------------------------------------

sub view {
	foreach my $i (<DATA>) {
		chomp $i;
		#print $thousand->{$pop}, "\n";

		#print "$pop\n";
		print "wget ftp://ftp-trace.ncbi.nih.gov/1000genomes/ftp/data/NA06984/exome_alignment/NA06984.mapped.ILLUMINAZZ.bwa.$pop.exome.*.bam\n";
	}
	return;
}
# wget ftp://ftp-trace.ncbi.nih.gov/1000genomes/ftp/data/NA06984/exome_alignment/NA06984.mapped.ILLUMINAZZ.bwa.CEU.exome.*.bam


##-----------------------------------------------------------------------------

=cut
sub reduceRunner {

	# make bam master file
	foreach my $file (@dirs) {
	    `find $file -name \"*bam\" >> bam_file.list`;
	}
	( scalar @dirs => 1 )
	  ? warn "Bam files found\n"
	  : die "Can't find bam files in given dir\n";

	# run RR
	my @bams = `cat bam_file.list` or die "could not read bam_file.list $!\n";
	foreach my $bam (@bams) {
	    chomp $bam;

	    next if ( $bam =~ /\.reduced\.bam$/ );
	    ( my $output = $bam ) =~ s/\.bam$/\.reduced\.bam/;
	    if ( -e $output and -s $output > 100 ) { next }

	    $pm->start and next;
	    my $cmd = "java -jar -Xmx60g -XX:ParallelGCThreads=3 -Djava.io.tmpdir=$tmp  $gatk "
		  #. "-T ReduceReads -R $fasta -I $bam -o $output &>> ReduceRunner.report";
		  . "-T ReduceReads -R /dev/shm/srynearson/human_g1k_v37_decoy.fasta -I $bam -o $output &>> ReduceRunner.report";
	    `$cmd`;
	    $pm->finish;
	}
	$pm->wait_all_children;
	`rm bam_file.list`;
}
##-----------------------------------------------------------------------------

sub wget {
	foreach my $wget (<DATA>) {
		chomp $wget;

		$pm->start and next;
		`$wget`;
		$pm->finish;
	}
	$pm->wait_all_children;
}
##-----------------------------------------------------------------------------

sub samIndex {
	opendir(DIR, '.');
	foreach my $bam (readdir(DIR)) {
		chomp $bam;
		next unless ( $bam =~ /\.bam$/);
		$pm->start and next;
		`samtools index $bam`;
		$pm->finish;
	}
	$pm->wait_all_children;
}
##-----------------------------------------------------------------------------
=cut

